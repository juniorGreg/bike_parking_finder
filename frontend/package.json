{
  "name": "frontend",
  "version": "0.1.0",
  "main": "main.js",
  "scripts": {
    "serve": "parcel watch src/main.js -d ../static --public-url=/static/",
    "build": "parcel build src/main.js -d ../static"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bulma": "^0.9.2",
    "leaflet": "^1.7.1",
    "vue": "^2.6.14",
    "vue-hot-reload-api": "^2.3.4",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.2.0",
    "cssnano": "^5.0.5",
    "postcss": "^8.3.0",
    "sass": "^1.34.1",
    "vue-template-compiler": "^2.6.14"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  },
  "staticFiles": {
    "staticPath": [
      {
        "outDirPattern": "../static",
        "staticPath": "fonts"
      }
    ],
    "watcherGlob": "**"
  }
}
